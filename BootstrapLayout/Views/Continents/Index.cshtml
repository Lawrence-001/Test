@model List<Continent>
@{
    ViewData["Title"] = "List of continents";
}
<div class=" row col-lg-4">

    <table class="table-bordered">
        <thead>
            <tr>
                <th>Code</th>
                <th>Continent Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var continent in Model)
            {
                <tr>
                    <td>@continent.Code</td>
                    <td>@continent.Name</td>
                </tr>
            }
        </tbody>

    </table>
</div>